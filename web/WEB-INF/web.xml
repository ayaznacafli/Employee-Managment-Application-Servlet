<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>UserFilter</filter-name>
        <filter-class>Filter.UserFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>UserFilter</filter-name>
        <url-pattern>/safe/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>HTML.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>elaveEt</servlet-name>
        <servlet-class>HTML.elaveEt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>elaveEtServlet</servlet-name>
        <servlet-class>HTML.elaveEtServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>duzenle</servlet-name>
        <servlet-class>HTML.duzenle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>duzenleServlet</servlet-name>
        <servlet-class>HTML.duzenleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sil</servlet-name>
        <servlet-class>HTML.sil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>silServlet</servlet-name>
        <servlet-class>HTML.silServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>HTML.search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>HTML.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>HTML.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registr</servlet-name>
        <servlet-class>HTML.registr</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrServlet</servlet-name>
        <servlet-class>HTML.registrServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>HTML.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>HTML.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>HTML.logoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>confirmRegistr</servlet-name>
        <servlet-class>Mail.ConfirmationRegistr</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>erorServlet</servlet-name>
        <servlet-class>Mail.erorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>elaveEt</servlet-name>
        <url-pattern>/safe/elaveEt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>elaveEtServlet</servlet-name>
        <url-pattern>/elaveEtServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>duzenle</servlet-name>
        <url-pattern>/safe/duzenle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>duzenleServlet</servlet-name>
        <url-pattern>/duzenleServlet</url-pattern>
    </servlet-mapping>
